{
    "name": "trenton93/think-wechat",
    "description": "EasyWechat For Thinkphp6.0",
    "type": "think-extend",
    "keywords": ["wechat", "weixin","easywechat","thinkphp","think", "sdk"],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "overtrue/wechat": "^5.0",
        "topthink/framework": "^6.0",
        "psr/simple-cache": "^1.0"
    },
    "authors": [{
        "name": "Trenton",
        "email": "370887237@qq.com"
    }],
    "autoload": {
        "psr-4": {
            "trenton93\\ThinkWechat\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "trenton93\\ThinkWechat\\WechatService"
            ],
            "config":{
                "wechat": "src/config.php"
            }
        }
    }
}